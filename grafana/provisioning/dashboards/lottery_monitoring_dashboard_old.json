{
  "id": null,
  "uid": "lottery-monitoring",
  "title": "Lottery Container Monitoring (OLD)",
  "tags": [
    "lottery",
    "containers"
  ],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "container",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "sort": 1,
        "query": "label_values(container_memory_usage_bytes{container_label_com_docker_compose_service=~\"lottery_.*\"}, container_label_com_docker_compose_service)",
        "definition": "label_values(container_memory_usage_bytes{container_label_com_docker_compose_service=~\"lottery_.*\"}, container_label_com_docker_compose_service)",
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "CPU Usage (%)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=~\"$container\"}[5m]) * 100",
          "legendFormat": "{{container_label_com_docker_compose_service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 9
      }
    },
    {
      "type": "timeseries",
      "title": "Memory Usage (MB)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "container_memory_usage_bytes{container_label_com_docker_compose_service=~\"$container\"} / 1024 / 1024",
          "legendFormat": "{{container_label_com_docker_compose_service}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "megabytes"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 9,
        "w": 24,
        "h": 9
      }
    },
    {
      "type": "timeseries",
      "title": "Network Transmit (bytes/sec)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(container_network_transmit_bytes_total{container_label_com_docker_compose_service=~\"$container\"}[5m])",
          "legendFormat": "{{container_label_com_docker_compose_service}}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 18,
        "w": 24,
        "h": 9
      }
    },
    {
      "type": "timeseries",
      "title": "Network Receive (bytes/sec)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{container_label_com_docker_compose_service=~\"$container\"}[5m])",
          "legendFormat": "{{container_label_com_docker_compose_service}}",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 27,
        "w": 24,
        "h": 9
      }
    },
    {
      "type": "timeseries",
      "title": "Filesystem Usage (Bytes)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "container_fs_usage_bytes{container_label_com_docker_compose_service=~\"$container\"}",
          "legendFormat": "{{container_label_com_docker_compose_service}}",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 36,
        "w": 24,
        "h": 9
      }
    },
    {
      "type": "timeseries",
      "title": "Container Uptime (seconds)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "time() - container_start_time_seconds{container_label_com_docker_compose_service=~\"$container\"}",
          "legendFormat": "{{container_label_com_docker_compose_service}}",
          "refId": "F"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 45,
        "w": 24,
        "h": 9
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": []
  }
}